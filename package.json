{
  "name": "zoomifyjs",
  "version": "0.2.0",
  "description": "lightweight plugin for image zoom functionality",
  "main": "./dist/zoomifyjs.mjs",
  "type": "module",
  "scripts": {
    "build": "./make.sh",
    "dev": "NODE_ENV=dev vite build --watch",
    "docs:preview": "generate-md --layout ./docs/_layouts/ --input ./docs/ --output ./output/",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdown-styles": "^3.2.0",
    "vite": "^4.4.4"
  },
  "repository": "mrazinshaikh/zoomifyjs",
  "author": "M Razin Shaikh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrazinshaikh/zoomifyjs/issues"
  },
  "homepage": "https://mrazinshaikh.github.io/zoomifyjs"
}
