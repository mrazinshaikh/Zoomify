{
  "name": "zoomifyjs",
  "version": "0.1.2",
  "description": "lightweight plugin for image zoom functionality",
  "main": "./dist/zoomifyjs.mjs",
  "scripts": {
    "build": "eslint --fix src/zoomifyJs.js && vite build && cp dist/zoomifyjs.umd.js docs/assets/zoomifyjs.umd.js",
