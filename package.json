  "devDependencies": {
    "eslint": "4.16.0",
    "eslint-loader": "^1.9.0",
    "markdown-styles": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^4.4.4"
  },
  "repository": "mrazinshaikh/zoomifyjs",
